{
  "name": "mullerpeter-databricks-datasource",
  "private": true,
  "version": "1.2.3",
  "description": "Databricks SQL Connector",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "start": "yarn watch",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit"
  },
  "author": "Peter MÃ¼ller",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@grafana/e2e": "10.4.0",
    "@grafana/e2e-selectors": "10.4.0",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@swc/core": "1.3.75",
    "@swc/helpers": "^0.5.3",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.9.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^10.3.10",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.1.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.69.5",
    "sass-loader": "13.3.2",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
